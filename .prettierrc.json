{
  "$schema": "https://json.schemastore.org/prettierrc",
  "attributeGroups": [
    "^v-if$",
    "^v-else$",
    "^v-else-if$",
    "^v-for$",
    "^v-show$",
    "^(?::)?id$",
    "^(?::)?ref",
    "^(?::)?key",
    "^v-(?!loading$!if$!else$!else-if$!for$)",
    "^v-loading$",
    "^v-slot$",
    "^v-model$",
    "^(?::)?model",
    "^(?::)?class$",
    "^(?::)?name$",
    "^(?::)?label$",
    "^(?::)?lang$",
    "^(?::)?src$",
    "^(?::)?type$",
    "^(?::)?href$",
    "^(?::)?value$",
    "^(?::)?title$",
    "^(?::)?alt$",
    "^(?::)?placeholder$",
    "$DEFAULT",
    "^(?::)?disabled$",
    "^(?::)?loading$",
    "^(?::)?src$",
    "^@"
  ],
  "attributeSort": "ASC",
  "pluginSearchDirs": false,
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-packagejson",
    "prettier-plugin-organize-attributes",
    "prettier-plugin-tailwindcss"
  ],
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none"
}
